#ifndef INC_MOTOR_DRIVER_H_
#define INC_MOTOR_DRIVER_H_

#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>

typedef struct motor {
	uint32_t duty_cycle;
	int8_t direction;
} motor;

/
void motor_set(motor* m, uint32_t duty_cycle, int8_t direction);

#endif /* INC_MOTOR_DRIVER_H_ */
